android_app {
    name: "XiaomiDolby",

    srcs: ["src/**/*.kt"],
    resource_dirs: ["res"],
    certificate: "platform",
    platform_apis: true,
    system_ext_specific: true,
    privileged: true,

    overrides: ["MusicFX", "AudioFX"],
    static_libs: [
        "SettingsLib",
        "SpaLib",
        "androidx.activity_activity-compose",
